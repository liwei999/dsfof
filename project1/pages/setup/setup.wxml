<view class="settings">
  <view class="settings-list">
    <view class="settings-item">
      <text class="settings-text">客服QQ</text>
      <text class="settings-desc">{{QQ}}</text>
    </view>
    <view class="settings-item no-border">
      <text class="settings-text">E-mail</text>
      <text class="settings-desc">{{version}}</text>
    </view>
  </view>
  <view class="settings-list">
    <view class="settings-item">
      <navigator url='../password/password' class="settings-text">修改密码</navigator>
    </view>
    <view bindtap="unsubscribeEvent" class="settings-item no-border">
      <text class="settings-text">退出登录</text>
    </view>
  </view>
</view>